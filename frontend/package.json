{
  "name": "trunfo",
  "version": "1.0.0",
  "description": "Front-end part of project Trunfo",
  "author": "Julio L. Muller",
  "license": "MIT",
  "homepage": "https://juliolmuller.github.io/trunfo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/trunfo.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/trunfo/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --dest ../docs",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@lacussoft/eslint-config-standard": "^1.0.2",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-router": "^4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.3.0",
    "@vue/cli-plugin-vuex": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.33",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.25",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@lacussoft/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
